import numpy as np
month = ['쌍', '쇠','복','돌','팽','육','쌍','개','칠','갑','삼','방']
month
day = ['봉', '구', '욕', '포', '똥', '삼', '식', '석', '놈', '님', '년', '돌', '단', '득', '방', '질', '장', '걸', '래', '룡', '동', '순', '자', '박', '창', '언', '것', '포', '만', '단', '국']
day
def get_my_chosun_name(name:str,a:int,b:int):
    change_name=name+month[a-1]+day[b-1]
    print(f"저의 이름은 {change_name} 입니다")
get_my_chosun_name("박",4, 6)
